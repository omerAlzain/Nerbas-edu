    <section t-name="Visitor" class="page-section container" id="contact">
        <div class="row">
            <div class="col-md-12 col-lg-5">
                <VisitorForm onAddedNewForm="onAddedNewForm"/>
            </div>
            <div class="col-md-12 col-lg-7">
                <VisitorList visitors="state.visitors" />
            </div>
        </div>
    </section>
    <div t-name="VisitorForm"  class="px-1 px-lg-2">
        <div class="row gx-1 gx-lg-2 justify-content-center">
            <div class="col-lg-8 col-xl-6 text-center">
                <h2 class="mt-0">Let's Get In Touch!</h2>
                <hr class="divider" />
                <p class="text-muted mb-5">Ready to start your next project with us? Send us a messages and we will get back to you as soon as possible!</p>
            </div>
        </div>
        <div class="row gx-1 gx-lg-1 justify-content-center mb-5">
            <div>
                <form id="contactForm">
                    <!-- Name input-->
                    <div class="form-floating mb-3">
                        <input class="form-control" id="name" type="text" placeholder="Enter your name..." t-ref="name" />
                        <label for="name">Full name</label>
                        <div class="invalid-feedback" data-sb-feedback="name:required">A name is required.</div>
                    </div>
                    <!-- Email address input-->
                    <div class="form-floating mb-3">
                        <input class="form-control" id="email" type="email" placeholder="name@example.com" t-ref="email" />
                        <label for="email">Email address</label>
                        <div class="invalid-feedback" data-sb-feedback="email:required">An email is required.</div>
                        <div class="invalid-feedback" data-sb-feedback="email:email">Email is not valid.</div>
                    </div>
                    <!-- Phone number input-->
                    <div class="form-floating mb-3">
                        <input class="form-control" id="phone" type="tel" placeholder="(123) 456-7890" t-ref="phone" />
                        <label for="phone">Phone number</label>
                        <div class="invalid-feedback">A phone number is required.</div>
                    </div>
                    <!-- Message input-->
                    <div class="form-floating mb-3">
                        <textarea class="form-control" id="message" type="text" placeholder="Enter your message here..." t-ref="message"></textarea>
                        <label for="message">Message</label>
                        <div class="invalid-feedback">A message is required.</div>
                    </div>
                    <!-- Submit success message-->
                    <!---->
                    <!-- This is what your users will see when the form-->
                    <!-- has successfully submitted-->
                    <div t-if=" state.submitted" id="submitSuccessMessage">
                        <div class="text-center mb-3">
                            <div class="fw-bolder">Form submission successful!</div>
                        </div>
                    </div>
                    <!-- Submit error message-->
                    <!---->
                    <!-- This is what your users will see when there is-->
                    <!-- an error submitting the form-->
                    <div t-if="state.error" id="submitErrorMessage"><div class="text-center text-danger mb-3"><t t-esc="state.error"/> </div></div>
                    <div t-if="!state.valid" id="submitErrorMessage"><div class="text-center text-danger mb-3">All fields are required!</div></div>
                    <!-- Submit Button-->
                    <div class="d-grid"><button class="btn btn-primary btn-xl" t-att-class="{disabled: state.submitted || state.error}" id="submitButton" type="submit" t-on-click="onSubmit">Submit</button></div>
                </form>
            </div>
        </div>
        <div class="row gx-1 gx-lg-2 justify-content-center">
            <div class="col-lg-4 text-center mb-5 mb-lg-0">
                <i class="bi-phone fs-2 mb-3 text-muted"></i>
                <div>phone number will be available soon</div>
            </div>
        </div>
    </div>
    <div t-name="VisitorList"  class="px-1 px-lg-1">
        <div class="row gx-1 gx-lg-2 justify-content-center">
            <div class="col-lg-8 col-xl-6 text-center">
                <h2 class="mt-0">Our Visitors!</h2>
                <hr class="divider" />
            </div>
        </div>
        <div class="row gx-1 gx-lg-1 justify-content-center">
            <div class="col-3"><h5>Name</h5></div>
            <div class="col-4"><h5>Email</h5></div>
            <div class="col-2"><h5>Phone</h5></div>
            <div class="col-3"><h5>Message</h5></div>
        </div>
        <div t-foreach="props.visitors" t-as="visitor" t-key="visitor.pk" class="row gx-1 gx-lg-1 justify-content-center">
            <div class="col-3 text-wrap mb-1 badge bg-secondary text-wrap"><t t-esc="visitor.name" /></div>
            <div class="col-4 mb-2 badge bg-primary text-wrap" ><t t-esc="visitor.email" /></div>
            <div class="col-2 text-wrap mb-2 badge bg-secondary text-wrap"><t t-esc="visitor.phone" /></div>
            <div class="col-3 text-wrap mb-2 badge bg-secondary text-wrap"><t t-esc="visitor.message" /></div>
        </div>
    </div>
